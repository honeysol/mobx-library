{
  "name": "mobx-resource-cache",
  "version": "1.0.0-alpha.15",
  "description": "Resource cache based on MobX, which supports an automatic genaration and clean-up of resource, lazy clean-up and a customized clean-up function",
  "author": "norami",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/honeysol/mobx-library.git",
    "directory": "packages/mobx-resource-cache"
  },
  "scripts": {
    "start": "tsdx watch --tsconfig tsconfig.build.json --verbose --noClean",
    "build": "tsdx build --tsconfig tsconfig.build.json",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepublish": "npm run build"
  },
  "main": "dist/index.js",
  "module": "dist/mobx-resource-cache.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "peerDependencies": {
    "mobx": ">=5"
  },
  "devDependencies": {
    "mobx": "^6.1.8",
    "mobx-monitor": "^1.0.0-alpha.15"
  },
  "dependencies": {
    "mobx-demand": "^1.0.0-alpha.15"
  },
  "publishConfig": {
    "access": "public"
  }
}
