{
  "name": "mobx-memo",
  "version": "1.0.0-alpha.13",
  "description": "Implementation of memoizer based on MobX, which supports an automatic genaration and clean-up of resource, lazy clean-up and a customized clean-up function",
  "author": "norami",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/honeysol/mobx-library.git",
    "directory": "packages/mobx-memo"
  },
  "scripts": {
    "start": "tsdx watch --tsconfig tsconfig.build.json --verbose --noClean",
    "build": "tsdx build --tsconfig tsconfig.build.json",
    "test": "jest",
    "lint": "tsdx lint",
    "prepublish": "npm run build"
  },
  "main": "dist/index.js",
  "module": "dist/mobx-memo.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "peerDependencies": {
    "mobx": ">=5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "jest": "^26.6.3",
    "mobx": "^6.1.8",
    "mobx-monitor": "^1.0.0-alpha.13",
    "ts-jest": "^26.5.5"
  },
  "dependencies": {
    "mobx-annotation-manipulator": "^1.0.0-alpha.13",
    "mobx-demand": "^1.0.0-alpha.13"
  },
  "publishConfig": {
    "access": "public"
  }
}
